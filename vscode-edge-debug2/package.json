{
  "name": "vscode-edge-debug2",
  "displayName": "Debugger for Edge",
  "version": "1.0.7",
  "icon": "images/icon.png",
  "description": "%extension.description%",
  "author": {
    "name": "Microsoft Corporation"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-edge-debug2"
  },
  "publisher": "msjsdiag",
  "bugs": "https://github.com/microsoft/vscode-edge-debug2/issues",
  "engines": {
    "vscode": "^1.21.0"
  },
  "categories": [
    "Debuggers"
  ],
  "keywords": [
    "multi-root ready"
  ],
  "license": "SEE LICENSE IN LICENSE.txt",
  "dependencies": {
    "portscanner": "^2.2.0",
    "vscode-chrome-debug-core": "^6.7.36",
    "vscode-debugadapter": "^1.31.0",
    "vscode-nls": "^3.2.2"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/mockery": "^1.4.29",
    "@types/node": "^6.0.110",
    "@types/source-map": "^0.1.27",
    "@types/tmp": "0.0.32",
    "concurrently": "^3.1.0",
    "event-stream": "^3.3.4",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^4.0.2",
    "gulp-util": "^3.0.8",
    "http-server": "^0.10.0",
    "mocha": "^5.2.0",
    "mockery": "^1.7.0",
    "run-sequence": "^2.2.0",
    "tmp": "0.0.31",
    "ts-loader": "^1.0.0",
    "tslint": "^5.10.0",
    "typemoq": "^2.1.0",
    "typescript": "^2.8.3",
    "vsce": "^1.40.0",
    "vscode": "^1.1.21",
    "vscode-chrome-debug-core-testsupport": "^3.21.0",
    "vscode-debugadapter-testsupport": "1.24.0",
    "vscode-debugprotocol": "^1.31.0-pre.0",
    "vscode-nls-dev": "^3.0.9"
  },
  "main": "./out/src/extension",
  "activationEvents": [
  ],
  "scripts": {
    "build": "gulp build",
    "watch": "gulp watch",
    "start": "node out/src/edgeDebug.js --server=4712",
    "test": "mocha --exit --timeout 20000 -s 2000 -u tdd --colors \"./out/test/*.test.js\"",
    "intTest": "mocha --exit --timeout 20000 -s 3500 -u tdd --colors --reporter node_modules/vscode-chrome-debug-core-testsupport/out/loggingReporter.js ./out/test/int/*.test.js",
    "lint": "gulp tslint",
    "vscode:prepublish": "gulp verify-no-linked-modules",
    "package": "gulp package"
  },
  "contributes": {
    "breakpoints": [
      {
        "language": "javascript"
      },
      {
        "language": "typescriptreact"
      },
      {
        "language": "javascriptreact"
      },
      {
        "language": "fsharp"
      }
    ],
    "menus": {
      "debug/callstack/context": [
        {
          "command": "extension.edge-debug.toggleSkippingFile",
          "group": "navigation",
          "when": "inDebugMode && debugType == 'edge'"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.edge-debug.toggleSkippingFile",
        "title": "%edge.toggleSkipping.title%"
      }
    ],
    "keybindings": [
      {
        "command": "extension.node-debug.pickLoadedScript",
        "key": "ctrl+f4",
        "mac": "cmd+f4",
        "when": "debugType == 'edge'"
      }
    ]
  }
}
